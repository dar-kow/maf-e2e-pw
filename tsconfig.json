{
    "compilerOptions": {
      "target": "ESNext",
      "module": "CommonJS",
      "moduleResolution": "Node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": ".",
      "paths": {
        "@navbar/*": ["tests/navbar/*"],
        "@sidebar/*": ["tests/sidebar/*"],
        "@utils/*": ["utils/*"],
      }
    },
    "include": ["tests", "utils"]
  }